    "@rollup/plugin-typescript": "^11.1.6",
    "@types/lodash": "^4.17.0",
    "body-parser": "^1.20.3",
    "dayjs": "^1.11.13",
    "express": "^4.21.1",
    "google-protobuf": "^3.21.4",
    "hapydev-runtime": "^2.0.12",
    "json5": "^2.2.3",
    "lodash": "^4.17.21",
    "socket.io": "^4.8.1",
    "strip-json-comments": "^3.1.1",
